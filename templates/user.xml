<templateSet group="user">
  <template name="form-text" value="&lt;!-- $NAME$ Form Input --&gt;&#10;&lt;div class=&quot;form-group&quot;&gt;&#10;    {!! Form::label('$NAME$','$VALUE$:') !!}&#10;    {!! Form::text('$NAME$', null, ['class'=&gt;'form-control','placeholder'=&gt;'$VALUE$']) !!}&#10;&lt;/div&gt;" description="Laravel form textfield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="form-submit" value="&lt;!-- $VALUE$ Form Input --&gt;&#10;        &lt;div class=&quot;form-group&quot;&gt;&#10;            {!! Form::submit('$VALUE$', ['class'=&gt;'btn btn-success form-control']) !!}&#10;        &lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="bsrow" value="&lt;div class=&quot;row&quot;&gt;&#10;            &#10;&lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="bscol" value="            &lt;div class=&quot;col-$COL$&quot;&gt;&#10;&#10;            &lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="COL" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="form-open" value="{!! Form::open(['action'=&gt;'$ACTION$']) !!}" description="" toReformat="true" toShortenFQNames="true">
    <variable name="ACTION" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="form-close" value="{!! Form::close() !!}" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="form-select" value="&lt;!-- $NAME$ Form Input --&gt;&#10; &lt;div class=&quot;form-group&quot;&gt;&#10;    {!! Form::label('$NAME$','$VALUE$:') !!}    &#10;    {!! Form::select('$NAME$', $OPTIONS$); !!}&#10; &lt;/div&gt;" description="" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="capitalize(String)" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="OPTIONS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML" value="true" />
      <option name="JSP" value="false" />
    </context>
  </template>
  <template name="pvf" value="private function $NAME$($PARAMS$)&#10;{&#10;    $END$&#10;}" description="Private function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
      <option name="PHP Comment" value="false" />
      <option name="PHP String Literal" value="false" />
    </context>
  </template>
  <template name="prf" value="protected function $NAME$($PARAMS$)&#10;{&#10;    $END$&#10;}" description="Protected function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
      <option name="PHP Comment" value="false" />
      <option name="PHP String Literal" value="false" />
    </context>
  </template>
  <template name="pbf" value="private function $NAME$($PARAMS$)&#10;{&#10;    $END$&#10;}" description="Public function" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSP" value="false" />
      <option name="PHP" value="true" />
      <option name="PHP Comment" value="false" />
      <option name="PHP String Literal" value="false" />
    </context>
  </template>
  <template name="&lt;abbreviation&gt;" value="&lt;template&gt;&#10;    &#10;&lt;/template&gt;&#10;&#10;&lt;style scoped&gt;&#10;&#10;&lt;/style&gt;&#10;&#10;&lt;script type=&quot;text/babel&quot;&gt;&#10;    export default {&#10;        props: [],&#10;&#10;        data() {&#10;            return {&#10;                &#10;            }&#10;        },&#10;        &#10;        created() {&#10;            &#10;        },&#10;        &#10;        methods: {&#10;&#10;        },&#10;        &#10;        computed: {&#10;            &#10;        }&#10;    }&#10;&lt;/script&gt;&#10;" description="" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
    </context>
  </template>
</templateSet>